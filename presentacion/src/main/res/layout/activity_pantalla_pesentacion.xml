<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/fondo_principal"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:theme="@style/Theme.Estacionamiento">

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/logo_nube"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margen_xxl"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/nube" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/letrero_app"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/margen_media"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_estacionamiento" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/imagen_inicial_parqueadero"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/margen_xxl"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/logo_nube"
        app:srcCompat="@drawable/images" />

</androidx.constraintlayout.widget.ConstraintLayout>